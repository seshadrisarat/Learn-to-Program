<%@page import="team.Team"%>
<%@page import="team.TeamBO"%>
<%@page import="java.util.List"%>
<%
	List<Team> teams = new TeamBO().obtainAllTeams();
%>

<html>
	<body>
		<h2>Existing Teams</h2>
		<br>
		<table id="teams">
			<tr>
				<th>Team</th>
				<th>Coach</th>
			</tr>
			<%
				for(Team t: teams) {
					out.print("<tr><td>""</td><td>Coach</td></tr>");
				}
			%>
		</table>
	</body>
</html>